<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dinesh.hungervalleyadmin">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".SingleCompletedActivity"></activity>

        <activity android:name=".OrdersByDeliveryBoy"
            android:parentActivityName=".DelvieryBoysActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".DelvieryBoysActivity"
            android:parentActivityName=".MyActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".EditCategory" />
        <activity
            android:name=".AddNewCategory"
            android:parentActivityName=".CategoriesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AddNewCatItem"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SubCategory"
            android:parentActivityName=".CategoriesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CategoriesActivity"
            android:parentActivityName=".MyActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".AddItem" />
        <activity
            android:name=".SingleRestaurant"
            android:parentActivityName=".Restaurants"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Restaurants"
            android:parentActivityName=".MyActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".EditOrder"
            android:screenOrientation="portrait" />
        <activity android:name=".OrdersActivity" />
        <activity
            android:name=".NewOrdersActivity"
            android:parentActivityName=".MyActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SingleOrderActivity"
            android:parentActivityName=".NewOrdersActivity" />
        <activity android:name=".MyActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>